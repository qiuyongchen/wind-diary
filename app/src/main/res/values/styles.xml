<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/c_colorPrimary</item>
        <item name="colorPrimaryDark">@color/c_colorPrimaryDark</item>
        <item name="colorAccent">@color/c_colorAccent</item>
        <item name="mainTextColor">#000000</item>
        <item name="textString">默认主题</item>
    </style>

    <!-- Night mode theme. -->
    <style name="AppTheme.Night" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryDark">@color/white</item>
        <item name="colorAccent">@color/white</item>
        <item name="mainTextColor">#000000</item>
        <item name="textString">默认主题</item>
    </style>

    <style name="Alp_42447968"></style>

    <style name="Alp_42447968.BaseThemeResources" parent="@style/Alp_42447968">
        <item name="alp_42447968_button_bar_style">@null</item>
        <item name="alp_42447968_button_bar_button_style">@null</item>
    </style>

    <!-- THEME RESOURCES DARK -->

    <style name="Alp_42447968.BaseThemeResources.Dark" parent="@style/Alp_42447968.BaseThemeResources"></style>

    <style name="Alp_42447968.ThemeResources.Dark" parent="@style/Alp_42447968.BaseThemeResources.Dark">

        <!-- AOSP -->

        <item name="android:windowBackground">@color/alp_42447968_aosp_background_material_dark
        </item>

        <!-- android-lockpattern -->

        <item name="alp_42447968_theme_dialog">@style/Alp_42447968.Theme.Dialog.Dark</item>
        <item name="alp_42447968_selector_ic_action_lockpattern">
            @drawable/alp_42447968_selector_ic_action_lockpattern_light
        </item>
        <item name="alp_42447968_color_lock_pattern_view_regular">
            @color/alp_42447968_lock_pattern_view_regular_light
        </item>
        <item name="alp_42447968_color_lock_pattern_view_success">
            @color/alp_42447968_lock_pattern_view_success_light
        </item>
        <item name="alp_42447968_color_lock_pattern_view_error">
            @color/alp_42447968_lock_pattern_view_error_light
        </item>
    </style>

    <!-- THEME RESOURCES LIGHT -->

    <style name="Alp_42447968.BaseThemeResources.Light" parent="@style/Alp_42447968.BaseThemeResources"></style>

    <style name="Alp_42447968.ThemeResources.Light" parent="@style/Alp_42447968.BaseThemeResources.Light">

        <!-- AOSP -->

        <item name="android:windowBackground">@color/alp_42447968_aosp_background_material_light
        </item>

        <!-- android-lockpattern -->

        <item name="alp_42447968_theme_dialog">@style/Alp_42447968.Theme.Dialog.Light</item>
        <item name="alp_42447968_selector_ic_action_lockpattern">
            @drawable/alp_42447968_selector_ic_action_lockpattern_dark
        </item>
        <item name="alp_42447968_color_lock_pattern_view_regular">
            @color/alp_42447968_lock_pattern_view_regular_dark
        </item>
        <item name="alp_42447968_color_lock_pattern_view_success">
            @color/alp_42447968_lock_pattern_view_success_dark
        </item>
        <item name="alp_42447968_color_lock_pattern_view_error">
            @color/alp_42447968_lock_pattern_view_error_dark
        </item>
    </style>

    <!-- THEME RESOURCES DIALOG DARK -->

    <style name="Alp_42447968.BaseThemeResources.Dialog" parent="@style/Alp_42447968.BaseThemeResources"></style>

    <style name="Alp_42447968.BaseThemeResources.Dialog.Dark" parent="@style/Alp_42447968.BaseThemeResources.Dialog"></style>

    <style name="Alp_42447968.ThemeResources.Dialog.Dark" parent="@style/Alp_42447968.BaseThemeResources.Dialog.Dark">

        <!-- AOSP -->

        <item name="android:windowBackground">@drawable/alp_42447968_aosp_dialog_full_holo_dark
        </item>

        <!-- android-lockpattern -->

        <item name="alp_42447968_theme_dialog">@style/Alp_42447968.Theme.Dialog.Dark</item>
        <item name="alp_42447968_selector_ic_action_lockpattern">
            @drawable/alp_42447968_selector_ic_action_lockpattern_light
        </item>
        <item name="alp_42447968_color_lock_pattern_view_regular">
            @color/alp_42447968_lock_pattern_view_regular_light
        </item>
        <item name="alp_42447968_color_lock_pattern_view_success">
            @color/alp_42447968_lock_pattern_view_success_light
        </item>
        <item name="alp_42447968_color_lock_pattern_view_error">
            @color/alp_42447968_lock_pattern_view_error_light
        </item>
    </style>

    <!-- THEME RESOURCES DIALOG LIGHT -->

    <style name="Alp_42447968.BaseThemeResources.Dialog.Light" parent="@style/Alp_42447968.BaseThemeResources.Dialog">

        <!-- AOSP -->

        <item name="android:windowFrame">@null</item>
        <item name="android:windowTitleStyle">@style/Alp_42447968.AOSP_DialogWindowTitle</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:windowSoftInputMode">stateUnspecified|adjustPan</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowActionModeOverlay">true</item>
        <item name="android:windowCloseOnTouchOutside">true</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>

    <style name="Alp_42447968.ThemeResources.Dialog.Light" parent="@style/Alp_42447968.BaseThemeResources.Dialog.Light">

        <!-- AOSP -->

        <item name="android:windowBackground">@drawable/alp_42447968_aosp_dialog_full_holo_light
        </item>

        <!-- android-lockpattern -->

        <item name="alp_42447968_theme_dialog">@style/Alp_42447968.Theme.Dialog.Light</item>
        <item name="alp_42447968_selector_ic_action_lockpattern">
            @drawable/alp_42447968_selector_ic_action_lockpattern_dark
        </item>
        <item name="alp_42447968_color_lock_pattern_view_regular">
            @color/alp_42447968_lock_pattern_view_regular_dark
        </item>
        <item name="alp_42447968_color_lock_pattern_view_success">
            @color/alp_42447968_lock_pattern_view_success_dark
        </item>
        <item name="alp_42447968_color_lock_pattern_view_error">
            @color/alp_42447968_lock_pattern_view_error_dark
        </item>
    </style>

    <!-- THEME DARK -->

    <style name="Alp_42447968.BaseTheme.Dark" parent="@android:style/Theme" />

    <style name="Alp_42447968.Theme.Dark" parent="@style/Alp_42447968.BaseTheme.Dark">

        <!-- android-lockpattern -->

        <item name="alp_42447968_theme_resources">@style/Alp_42447968.ThemeResources.Dark</item>
    </style>

    <!-- THEME LIGHT -->

    <style name="Alp_42447968.BaseTheme.Light" parent="@android:style/Theme.Light" />

    <style name="Alp_42447968.Theme.Light" parent="@style/Alp_42447968.BaseTheme.Light">

        <!-- android-lockpattern -->

        <item name="alp_42447968_theme_resources">@style/Alp_42447968.ThemeResources.Light</item>
    </style>

    <!-- THEME LIGHT, DARK ACTION BAR -->

    <style name="Alp_42447968.BaseTheme.Light.DarkActionBar" parent="@style/Alp_42447968.Theme.Light"></style>

    <style name="Alp_42447968.Theme.Light.DarkActionBar" parent="@style/Alp_42447968.BaseTheme.Light.DarkActionBar">

        <!-- android-lockpattern -->

        <item name="alp_42447968_theme_resources">@style/Alp_42447968.ThemeResources.Light</item>
    </style>

    <!-- THEME DIALOG DARK -->

    <style name="Alp_42447968.BaseTheme.Dialog.Dark" parent="@android:style/Theme.Dialog"></style>

    <style name="Alp_42447968.Theme.Dialog.Dark" parent="@style/Alp_42447968.BaseTheme.Dialog.Dark">

        <!-- android-lockpattern -->

        <item name="alp_42447968_theme_resources">@style/Alp_42447968.ThemeResources.Dialog.Dark
        </item>
    </style>

    <!-- THEME DIALOG LIGHT -->

    <style name="Alp_42447968.BaseTheme.Dialog.Light" parent="@android:style/Theme.Light"></style>

    <style name="Alp_42447968.Theme.Dialog.Light" parent="@style/Alp_42447968.BaseTheme.Dialog.Light">

        <!-- android-lockpattern -->

        <item name="alp_42447968_theme_resources">@style/Alp_42447968.ThemeResources.Dialog.Light
        </item>
    </style>

    <!-- THEME DIALOG WHEN LARGE, LIGHT -->

    <style name="Alp_42447968.BaseTheme.DialogWhenLarge.Light" parent="@style/Alp_42447968.Theme.Light"></style>

    <style name="Alp_42447968.Theme.DialogWhenLarge.Light" parent="@style/Alp_42447968.BaseTheme.DialogWhenLarge.Light">

        <!-- android-lockpattern -->

        <item name="alp_42447968_theme_resources">@style/Alp_42447968.ThemeResources.Light</item>
    </style>

    <!-- THEME DIALOG WHEN LARGE, DARK -->

    <style name="Alp_42447968.BaseTheme.DialogWhenLarge.Dark" parent="@style/Alp_42447968.Theme.Dark"></style>

    <style name="Alp_42447968.Theme.DialogWhenLarge.Dark" parent="@style/Alp_42447968.BaseTheme.DialogWhenLarge.Dark">

        <!-- android-lockpattern -->

        <item name="alp_42447968_theme_resources">@style/Alp_42447968.ThemeResources.Dark</item>
    </style>

    <style name="Alp_42447968.AOSP_DialogWindowTitle" parent="@style/Alp_42447968">
        <item name="android:maxLines">1</item>
        <item name="android:scrollHorizontally">true</item>
        <item name="android:textAppearance">@android:style/TextAppearance.DialogWindowTitle</item>
    </style>

    <declare-styleable name="Alp_42447968.LockPatternView">

        <!--
             Aspect to use when drawing LockPatternView. Choices are "square"(default), "lock_width"
             or "lock_height"
        -->
        <attr name="alp_42447968_aspect" format="string" />
        <!-- Color to use when drawing LockPatternView paths. -->
        <attr name="alp_42447968_pathColor" format="color|reference" />
        <!-- The regular pattern color -->
        <attr name="alp_42447968_regularColor" format="color|reference" />
        <!-- The error color -->
        <attr name="alp_42447968_errorColor" format="color|reference" />
        <!-- The success color -->
        <attr name="alp_42447968_successColor" format="color|reference" />
    </declare-styleable>

    <attr name="alp_42447968_button_bar_style" format="reference" />
    <attr name="alp_42447968_button_bar_button_style" format="reference" />
    <attr name="alp_42447968_theme_dialog" format="reference" />
    <attr name="alp_42447968_theme_resources" format="reference" />

</resources>
